{
  "devDependencies": {
    "prettier": "^3.1.0",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^4.9.5"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "importOrder": [
      "^@core/(.*)$",
      "^@server/(.*)$",
      "^@ui/(.*)$",
      "^[./](?!.*\\.css$).*$",
      "^[./]"
    ]
  }
}
